    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
